[{"id": 6, "si": {"title": "Umetna inteligenca in inteligentni sistemi", "description": "<p>\u010clani programske skupine se ukvarjajo z raziskavami na naslednjih podro\u010djih:</p>\r\n<p>strojno u\u010denje in odkrivanje zakonitosti v podatkih;</p>\r\n<p>evolucijsko ra\u010dunanje in preiskovalni algoritmi;</p>\r\n<p>programiranje z omejitvami in kombinatori\u010dna optimizacija;</p>\r\n<p>metode kvalitativnega sklepanja;</p>\r\n<p>strojno u\u010denje v biomedicinski informatiki;</p>\r\n<p>agentni sistemi in semanti\u010dni splet.</p>\r\n<p>Vse raziskovalne teme so vseskozi motivirane z mo\u017enostmi uporabe in neposrednimi konkretnimi aplikacijami.</p>\r\n", "proj_news": null, "proj_events": null, "proj_research": null, "proj_publications": null, "client": "Javna agencija za raziskovalno dejavnost RS", "project_id": "", "project_link": "", "start_date": "2015-01-01 00:00:00", "end_date": "2020-12-31 00:00:00", "persons": [{"id": "630864", "role": "Sodelavec na projektu", "ime": "Zoran", "priimek": "Bosni\u0107"}, {"id": "630025", "role": "Sodelavec na projektu", "ime": "Ivan", "priimek": "Bratko"}, {"id": "630620", "role": "Mladi raziskovalec", "ime": "Niko", "priimek": "Colneri\u010d"}, {"id": "630891", "role": "Sodelavec na projektu", "ime": "Toma\u017e", "priimek": "Curk"}, {"id": "630717", "role": "Mladi raziskovalec", "ime": "Andrej", "priimek": "\u010copar"}, {"id": "630589", "role": "Sodelavec na projektu", "ime": "Janez", "priimek": "Dem\u0161ar"}, {"id": "630855", "role": "Sodelavec na projektu", "ime": "Matej", "priimek": "Guid"}, {"id": "630992", "role": "Mladi raziskovalec", "ime": "Toma\u017e", "priimek": "Ho\u010devar"}, {"id": "630730", "role": "Mladi raziskovalec", "ime": "Martin", "priimek": "Jakomin"}, {"id": "630309", "role": "Vodja delovnega paketa", "ime": "Igor", "priimek": "Kononenko"}, {"id": "630511", "role": "Sodelavec na projektu", "ime": "Matja\u017e", "priimek": "Kukar"}, {"id": "630993", "role": "Mladi raziskovalec", "ime": "Timotej